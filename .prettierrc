{
  "plugins": ["prettier-plugin-tailwindcss"],
  "singleQuote": false,
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": false,
  "printWidth": 80,
  "arrowParens": "avoid",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "endOfLine": "lf",
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "useTabs": false
}
